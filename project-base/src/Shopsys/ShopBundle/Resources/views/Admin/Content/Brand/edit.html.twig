{% extends '@ShopsysShop/Admin/Content/Brand/detail.html.twig' %}

{% block title %}- {{ 'Editing brand'|trans }} - {{ brand.name }}{% endblock %}
{% block h1 %}{{ 'Editing brand'|trans }} - {{ brand.name }}{% endblock %}

{% block brand_urls %}
    {{ form_row(form.brandData.urls, { label: 'URL addresses'|trans }) }}
{% endblock %}

{% block image %}
    <dl class="form-line">
        <dd>
            {{ image(brand, { size: 'original', height: 100 }) }}
        </dd>
    </dl>
{% endblock %}
