<div class="text-left">
    <div id="js-domain-icon-errors" class="in-message in-message--danger js-domain-icon-errors display-none">
        <ul class="in-message__list"></ul>
    </div>

    {{ form_start(form) }}
        {{ form_errors(form) }}
        <div>
            <dl class="form-line">
                <dt class="form-line__label">
                    {{ 'Existing icon'|trans }}:
                </dt>
                <dd>
                    {{ domainIcon(domain.id) }}
                </dd>
            </dl>

            {{ form_row(form.icon, { label: false}) }}

            <dl class="form-line">
                <dd>
                    {{ form_save(domain|default(null), form) }}
                </dd>
            </dl>
        </div>
    {{ form_end(form) }}
</div>
<div class="js-overlay-spinner display-none text-center">
    <i class="svg svg-gear" title="{{ 'Loading'|trans }}"></i>
</div>
